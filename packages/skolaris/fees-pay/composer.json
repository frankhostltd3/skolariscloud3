{
    "name": "skolaris/fees-pay",
    "description": "A Drop-in Laravel package to handle school fees payments for a multi-tenant SaaS.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Skolaris Cloud FeesPay",
            "email": "info@skolaris.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "barryvdh/laravel-dompdf": "^2.0|^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Skolaris\\FeesPay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skolaris\\FeesPay\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Skolaris\\FeesPay\\SkolarisFeesPayServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
